package variable;

public class Var2 {
	public static void main(String[] args) {
		String lang = "Java";
		
		System.out.println("Hello " + lang + "!");
		
		System.out.println(lang + " is Simple");
		System.out.println("안녕하세요");
		System.out.println("반갑습니다");
		System.out.println("");
		System.out.println(lang + "를 공부합니다");
	}
}
